using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

class Program
{
    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        long[][] arr = new long[n][];
        if (n >= 1)
        {
            arr[0] = new long[] { 1 };
        }
        if (n >= 2)
        {
            arr[1] = new long[] { 1, 1 };
        }

        for (int i = 2; i < n; i++)
        {
            arr[i] = new long[i + 1];
            arr[i][0] = 1;
            arr[i][i] = 1;

            for (int col = 1; col < i; col++)
            {
                arr[i][col] = arr[i - 1][col] + arr[i - 1][col - 1];
            }
        }
        for (int i = 0; i < arr.Length; i++)
        {
            Console.WriteLine(string.Join(" ", arr[i]));
        }
    }
}
