using System;

public class Program
{
    static void Main()
    {
        int firstSeconds = int.Parse(Console.ReadLine());
        int secondSeconds = int.Parse(Console.ReadLine());
        int thirdSeconds = int.Parse(Console.ReadLine());

        int sum = firstSeconds + secondSeconds + thirdSeconds;

        int minutes = sum / 60;
        int seconds = sum % 60;

        Console.WriteLine($"{minutes:0}:{seconds:00}");


    }
}
