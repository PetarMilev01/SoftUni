using System;

public class Program
{
    static void Main()
    {
        int annualFee = int.Parse(Console.ReadLine());

        double shoes = annualFee * 0.6;
        double clothes = shoes * 0.8;
        double ball = clothes / 4;
        double stuff = ball / 5;

        Console.WriteLine(annualFee+shoes+clothes+ball+stuff);
    }
}
