using System;
using System.Collections.Generic;
using System.Linq;

public class Program
{
    static void Main()
    {
        int number = int.Parse(Console.ReadLine());
        int sum = int.Parse(Console.ReadLine());

        while (number > sum)
        {
            sum += int.Parse(Console.ReadLine());
        }
        Console.WriteLine(sum);
    }
}
