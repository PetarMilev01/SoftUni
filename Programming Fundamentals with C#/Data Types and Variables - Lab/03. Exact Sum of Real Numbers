using System;
using System.Collections.Generic;
using System.Linq;

public class Program
{
    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        decimal sum = (decimal)0.0;

        for (int i = 0; i < n; i++)
        {
            sum += decimal.Parse(Console.ReadLine());
        }
        Console.WriteLine(sum);
    }
}
