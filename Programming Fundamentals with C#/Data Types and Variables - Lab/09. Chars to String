using System;
using System.Collections.Generic;
using System.Linq;

public class Program
{
    static void Main()
    {
        string chars = "";

        for (int i = 0; i < 3; i++)
        {
            chars += Console.ReadLine();
        }

        foreach (var ichar in chars)
        {
            Console.Write(ichar + "");
        }
    }
}
