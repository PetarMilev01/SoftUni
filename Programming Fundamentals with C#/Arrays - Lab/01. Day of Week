using System;

public class Program
{
    static void Main()
    {
        int wantedDay = int.Parse(Console.ReadLine());

        if (wantedDay<=0 || wantedDay>7)
        {
            Console.WriteLine($"Invalid day!");
            return;
        }
        string[] weekDays = new string [] 
        {
            "Monday",
            "Tuesday",
            "Wednesday",
            "Thursday",
            "Friday",
            "Saturday",
            "Sunday"
        };
        Console.WriteLine(weekDays[wantedDay-1]);
    }
}
