using System;

namespace Person
{
    public class StartUp
    {
        public static void Main()
        {
            int number = int.Parse(Console.ReadLine());
            int currentNum = 0;
            int sum = 0;

            while (number > 0)
            {
                currentNum = number % 10;
                number /= 10;
                sum += currentNum;
            }
            Console.WriteLine(sum);
        }
    }
}
