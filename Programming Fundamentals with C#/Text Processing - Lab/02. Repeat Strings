using System;
using System.Collections.Generic;
using System.Linq;

public class Program
{
    static void Main()
    {
        string[] input = Console.ReadLine().Split(' ').ToArray();

        foreach (var word in input)
        {
            for (int i = 0; i < word.Length; i++)
            {
                Console.Write(word);
            }
        }
    }
}
