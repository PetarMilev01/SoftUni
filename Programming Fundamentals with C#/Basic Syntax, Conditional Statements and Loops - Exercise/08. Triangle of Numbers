using System;
using System.Collections.Generic;
using System.Linq;

class Program
{
    static void Main()
    {
        int number = int.Parse(Console.ReadLine());

        for (int i = 1; i <= number; i++)
        {
            for (int c = 1; c <= i; c++)
            {
                Console.Write(i + " ");
            }
            Console.WriteLine();
        }
    }
}
